{
  "name": "kbone-ui",
  "version": "0.6.17",
  "description": "Weui component Library for kbone-ui",
  "main": "lib/index.js",
  "scripts": {
    "dev:web": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.config.js",
    "dev:mp": "cross-env NODE_ENV=development webpack --config build/webpack.mp.config.js --progress --hide-modules",
    "build:weui": "gulp compile --gulpfile examples/weui/gulpfile.js",
    "build:ui": "cross-env NODE_ENV=production webpack --config build/bin/webpack.common.js --progress --hide-modules",
    "build": "rimraf lib && npm run build:ui && npm run build:weui",
    "dev": "cross-env NODE_ENV=production webpack --config build/bin/webpack.common.js --progress --hide-modules --watch",
    "test": "jest --colors --config .jestrc.json",
    "coverage": "jest --colors --coverage  --config .jestrc.json",
    "cov": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wechat-miniprogram/kbone-ui.git"
  },
  "keywords": [
    "kbone",
    "kbone-ui",
    "vue",
    "weui",
    "kbone-ui-weui"
  ],
  "author": "wechat-miniprogram",
  "license": "MIT",
  "browserslist": [
    "ie > 9",
    "last 2 versions"
  ],
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "typings": "typings/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.7.7",
    "@vue/test-utils": "^1.0.0-beta.30",
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^9.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "globby": "^10.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mp-webpack-plugin": "^0.1.3",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "portfinder": "^1.0.13",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "reduce-loader": "^0.1.1",
    "rimraf": "^2.7.1",
    "sinon": "^8.0.2",
    "terser-webpack-plugin": "^2.2.1",
    "thread-loader": "^2.1.3",
    "url-loader": "^0.5.8",
    "vue-improve-loader": "^0.1.1",
    "vue-jest": "^4.0.0-beta.2",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.1.0"
  }
}
